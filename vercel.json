{
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/pkg/main/main.go"
        }
    ],
    "redirects": [
        {
            "source": "/:path*",
            "destination": "/pkg/main/main.go"
        }
    ],
    "build": {
        "env": {
            "GO_BUILD_FLAGS": "-ldflags '-s -w'"
        }
    }
}